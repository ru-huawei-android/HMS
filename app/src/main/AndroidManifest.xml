<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.huawei.hms.ads">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!--    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />-->
    <uses-permission android:name="com.huawei.appmarket.service.commondata.permission.GET_COMMON_DATA" />

    <!--    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>-->
    <!--    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>-->

    <application
        android:name="com.huawei.hms.ads.AdsApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning,UnusedAttribute">
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait"
            tools:ignore="LockedOrientationActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BannerActivity" />
        <activity android:name=".NativeActivity" />
        <activity android:name=".RewardActivity" />
        <activity android:name=".InterstitialActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ConsentActivity" />

        <meta-data
            android:name="com.huawei.hms.client.appid"
            android:value="appid=101611999" />
    </application>

</manifest>